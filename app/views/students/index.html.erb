<div class="row">
  <div class="col s12">
    <h3 class="center-align">Student Listing</h3>

  </div>
</div>

<div class="row">
  <% @students.each do |student| %>
    <div class="col s6 m6">
      <div class="card light-blue darken-3">
        <div class="card-content white-text">
          <span class="card-title">Name: <%= student.name.capitalize %></span>
          <span class="card-title">Email: <%= student.email %></span>

          <p>Course Enrollments: None</p>
        </div>
        <div class="card-action">
          <a href="#">View Profile</a>

        </div>
      </div>
    </div>
  <% end %>

</div>
